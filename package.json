{
  "name": "link",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 9999 -H 0.0.0.0",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 next build",
    "start": "next start -p 9999 -H 0.0.0.0",
    "lint": "next lint",
    "test": "vitest",
    "db:generate": "drizzle-kit generate:mysql",
    "db:push": "drizzle-kit push:mysql",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node --loader ts-node/esm scripts/seed.ts",
    "db:sync": "node --loader ts-node/esm scripts/sync-schema.ts",
    "db:setup": "npm run db:sync && npm run db:generate && npm run db:push",
    "prestart": "chcp 65001",
    "premigrate": "chcp 65001",
    "db:auto-push": "powershell -Command \"echo 0 | npx drizzle-kit push:mysql\" "
  },
  "dependencies": {
    "@auth/core": "^0.38.0",
    "@headlessui/react": "^2.2.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.9",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.29.3",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.487.0",
    "mysql2": "^3.14.0",
    "next": "^15.3.0",
    "next-auth": "^4.24.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.5",
    "react-hot-toast": "^2.5.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.14",
    "eslint": "^9.20.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.5.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^3.1.1"
  }
}
